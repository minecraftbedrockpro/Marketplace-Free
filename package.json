{
  "name": "meu-app-electron",
  "version": "1.0.0",
  "description": "Meu aplicativo Electron",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "com.seuapp.electron",
    "publish": [
      {
        "provider": "github",
        "owner": "minecraftbedrockpro",
        "repo": "Marketplace-Free"
      }
    ],
    "mac": {
      "category": "public.app-category.productivity",
      "icon": "icons/app-icon.icns"
    },
    "win": {
      "target": "nsis",
      "icon": "icons/app-icon.ico"
    },
    "linux": {
      "target": "AppImage",
      "icon": "icons/app-icon.png"
    }
  },
  "devDependencies": {
    "electron": "^34.0.0",
    "electron-builder": "^25.1.8",
    "electron-reload": "^2.0.0-alpha.1"
  },
  "author": "Higor Rocha",
  "license": "MIT",
  "dependencies": {
    "electron-updater": "^6.3.9"
  }
}
